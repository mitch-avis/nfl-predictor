{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: nfl-predictor",
            "type": "debugpy",
            "request": "launch",
            "program": "nfl_predictor/data_collection.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
    ]
}